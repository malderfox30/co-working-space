{
  "name": "pikthat-admin-web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "yarn check-types && yarn gen-firebase && vite build",
    "start": "vite preview --port 3000",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --fix",
    "lint-staged": "lint-staged",
    "lint-pass": "echo '\\033[4;32m♡ ' Format complete ♡' \\033[0m'",
    "gen:i18:keys": "ts-node --project scripts/tsconfig.json scripts/generateTranslationTypes.ts",
    "gen-graphql-types": "graphql-codegen -r dotenv/config --config codegen.yml",
    "gen-firebase": "ts-node --project scripts/tsconfig.json scripts/swEnvbuild.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.7",
    "@ant-design/pro-layout": "^6.31.4",
    "@apollo/client": "3.4.16",
    "@base-projects/web-uikit": "^1.7.13",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@sentry/integrations": "^6.16.0",
    "@sentry/react": "^6.16.0",
    "@sentry/tracing": "^6.16.0",
    "@types/lodash.uniqby": "^4.7.6",
    "antd": "^4.17.2",
    "antd-img-crop": "^3.16.0",
    "apollo-link-error": "^1.1.13",
    "aws-amplify": "^4.3.10",
    "axios": "^0.24.0",
    "bezier-easing": "^2.1.0",
    "dayjs": "^1.10.7",
    "firebase": "^9.6.0",
    "graphql": "^16.1.0",
    "i18next": "^21.5.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "less-vars-to-js": "^1.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.uniqby": "^4.7.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.3",
    "react-router-dom": "^6.0.2",
    "react-virtualized": "^9.22.3",
    "recharts": "^2.1.6",
    "tailwindcss": "^3.0.8",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@enouvo-packages/eslint-config-react": "^1.0.6",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/fragment-matcher": "^3.2.0",
    "@graphql-codegen/import-types-preset": "^2.1.8",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^16.11.12",
    "@types/prettier": "^2.4.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-virtualized": "^9.21.15",
    "@types/recharts": "^1.8.23",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vitejs/plugin-react": "^1.1.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.5.2",
    "vite": "^2.7.1",
    "vite-plugin-antd-dayjs": "^1.0.0",
    "vite-plugin-imp": "^2.0.10",
    "vite-plugin-sentry": "^1.0.12"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintIgnore": [
    "vite.config.ts",
    "scripts/*"
  ],
  "resolutions": {
    "react-virtualized": "git+https://git@github.com/remorses/react-virtualized-fixed-import.git#9.22.3"
  }
}
